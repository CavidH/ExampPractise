<section id=featured>
    <div class="container cardss mt-5">
        <div class="headcards d-flex  mt-5   align-items-center ">
            
            @foreach (var Featuredcard in Model.FeatureCards)
            {
                <div class="card col-4  " style="width: 20rem;">
                    <div class="card-body">
                        @Html.Raw(Featuredcard.IconLink)
                        <h5 class="card-title">@Featuredcard.Title</h5>
                        <p class="card-text">
                            @Featuredcard.Content
                        </p>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
